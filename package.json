{
  "name": "savvycart",
  "version": "1.0.0",
  "description": "eCommerce platform",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/savvyshell/SavvyCart.git"
  },
  "author": "Siavash Khan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/savvyshell/SavvyCart/issues"
  },
  "homepage": "https://github.com/savvyshell/SavvyCart#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.25.1",
    "colors": "^1.4.0",
    "concurrently": "^6.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemon": "^2.0.7"
  },
  "engines": {
    "node": ">=14.15.1"
  }
}
